<div id="page-wrapper">
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-6">
                        <div class="row">
                                <div class="col-lg-12">
                                    <h1 class="page-header">Courses and Rubrics</h1>


                            <table class="table">
                              <thead>
                                <tr>
                                    <th>Major</th> 
                                    <th>Course</th>        
                                    <th>Rubric</th>
                                </tr>
                            </thead>

                            <tr ng-repeat="data in courses">
                                    <td>{[{data.major}]}</td>
                                    <td>{[{data.course}]}</td>
                                    <td>{[{data.rubric.name}]}</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
